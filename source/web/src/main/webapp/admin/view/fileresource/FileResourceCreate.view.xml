<mvc:View
	controllerName="cardies.adminapp.controller.fileresource.FileResourceCreate"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.ui.layout.form"
	xmlns:smartField="sap.ui.comp.smartfield"
	xmlns:core="sap.ui.core"
	busyIndicatorDelay="0">
	<Page
		id="page"
		title="{i18n>Page.Tile.FileResouce.create}"
		showNavButton="true"
		navButtonPress="onNavBack"
		class="sapUiResponsiveContentPadding">
		<core:Fragment fragmentName="cardies.adminapp.view.fileresource.FileResourceEdit" type="XML"/>
		<footer>
			<Bar>
				<contentRight>
					<Button id="save" text="{i18n>Label.Save}" type="Emphasized" visible="true" press="onSavePress" />
					<Button id="cancel" text="{i18n>Label.Cancel}" type="Reject" visible="true" press="onCancelPress" />
				</contentRight>
			</Bar>
		</footer>
	</Page>
</mvc:View>
